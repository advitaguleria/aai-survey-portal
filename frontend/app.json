{
  "expo": {
    "name": "aai-survey-portal",
    "slug": "aai-survey-portal",
    "updates": {
      "enabled": false
    },
    "owner": "advitaguleria",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aai.surveyportal",
    
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to suggest nearby airports during registration.",
        "NSLocationAlwaysUsageDescription": "This app needs access to your location to suggest nearby airports during registration."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aai.surveyportal",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-image-picker",
      "expo-location"
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}